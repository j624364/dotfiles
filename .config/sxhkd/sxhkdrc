XF86AudioRaiseVolume
    amixer -D pulse sset Master 5%+

XF86AudioLowerVolume
    amixer -D pulse sset Master 5%-

XF86AudioMute
    amixer sset Master toggle

XF86AudioPlay
	playerctl play-pause

XF86AudioPause
	playerctl play-pause

XF86AudioNext
	playerctl next

XF86AudioPrev
	playerctl previous

